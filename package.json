{
  "name": "srh-db-vde",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "cf-typegen": "wrangler types",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "wrangler d1 migrations apply --local DB",
    "db:migrate-production": "drizzle-kit migrate",
    "deploy": "bun run build && wrangler deploy",
    "dev": "react-router dev",
    "typecheck": "bun run cf-typegen && react-router typegen && tsc -b"
  },
  "dependencies": {
    "drizzle-orm": "^0.44.2",
    "isbot": "^5.1.28",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.6.3"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.7.5",
    "@react-router/dev": "^7.6.3",
    "@tailwindcss/vite": "^4.1.11",
    "@types/node": "^24.0.6",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "drizzle-kit": "~0.31.4",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.8.3",
    "vite": "6",
    "vite-tsconfig-paths": "^5.1.4",
    "wrangler": "^4.22.0"
  }
}