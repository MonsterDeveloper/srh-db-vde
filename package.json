{
  "name": "srh-db-vde",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "cf-typegen": "wrangler types",
    "db:push": "drizzle-kit push",
    "db:generate": "bun --env-file .env.production drizzle-kit generate",
    "db:migrate": "bun --env-file .env.production drizzle-kit migrate",
    "deploy": "bun run build && wrangler deploy",
    "dev": "react-router dev",
    "update": "bunx npm-check-updates -ui --reject bun",
    "preview": "bun run build && vite preview",
    "typecheck": "bun run cf-typegen && react-router typegen && tsc -b",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.44.2",
    "isbot": "^5.1.28",
    "lucide-react": "^0.525.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.6.3",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.7.5",
    "@react-router/dev": "^7.6.3",
    "@tailwindcss/vite": "^4.1.11",
    "@types/node": "^24.0.6",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "better-sqlite3": "^12.1.1",
    "drizzle-kit": "~0.31.4",
    "tailwindcss": "^4.1.11",
    "tw-animate-css": "^1.3.4",
    "typescript": "^5.8.3",
    "vite": "6",
    "vite-tsconfig-paths": "^5.1.4",
    "wrangler": "^4.22.0"
  }
}